# Required
UPSTREAM_BEARER=REPLACE_WITH_CYBERR_API_TOKEN

# Optional
UPSTREAM_URL=https://app.cyberr.ai/backend/api/v1/jobs
PORT=8080

# Security
REQUIRE_API_KEY=false
MIDDLEWARE_API_KEY=change-me
API_KEY_HEADER=x-internal-token

# CORS allow-list (comma-separated). Example:
# CORS_ORIGINS=https://example.com
CORS_ORIGINS=https://cybersecurity.lu,https://test.cybersecurity.lu

# Performance
CACHE_TTL_SECONDS=300
UPSTREAM_TIMEOUT_MS=8000
RATE_LIMIT_PER_MINUTE=60
LOG_LEVEL=info